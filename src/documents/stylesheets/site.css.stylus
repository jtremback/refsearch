html {
	font-size: 16px;
}

.search {
	font-size: 2em;
	border: 1px solid #aaa;
	outline: none;
}

#func-list {
	.list {
		list-style-type: none;
		margin: 0;
		padding: 0;
	}
}

.property {
	color: red;
}

.object_base {
	color: green;
}

.method {
	color: orange;
}

.result {
	.fingerprint {
		font-family: monospace;
		font-size: 1.2em;
	}
	.library {
		font-family: sans-serif;
	}
}